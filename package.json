{
  "name": "web-tj",
  "description": "web-tj",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p start:dev watch:css",
    "build": "npm run build:css && sapper build --legacy",
    "watch:css": "postcss src/tailwind.pcss -o static/index.css -w",
    "build:css": "NODE_ENV=production postcss src/tailwind.pcss -o static/index.css",
    "export": "npm run build:css && sapper export --legacy --entry '/ /404'",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^2.14.0",
    "dompurify": "^2.0.12",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.0",
    "polka": "next",
    "sapper-google-analytics": "^1.0.2",
    "siema": "^1.5.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@beyonk/svelte-mapbox": "^4.3.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tailwindcss/typography": "^0.2.0",
    "autoprefixer": "^9.8.5",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.2",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.28.0",
    "sitemap": "^6.2.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.0.6",
    "tailwindcss": "^1.6.0",
    "tiny-glob": "^0.2.6"
  }
}
